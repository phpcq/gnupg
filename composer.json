      "name": "Stefan Heimes",
      "email": "stefan_heimes@hotmail.com",
      "role": "lead"
    }
  ],
  "require": {
    "php": "^7.3 || ^8.0",
    "ext-dom": "*",
    "symfony/process": "^4.0 || ^5.0"
  },
  "conflict": {
    "psr/http-client": ">=2.0",
    "psr/http-factory": ">=2.0",
    "psr/http-message": ">=2.0"
  },
  "require-dev": {
    "phpcq/runner-bootstrap": "^1.0@dev",
    "phpspec/phpspec": "^6.2",
    "psr/http-client": "^1.0",
    "psr/http-factory": "^1.0",
    "psr/http-message": "^1.0"
  },
  "suggest": {
    "psr/http-client": "Require to use the provided HttpClientDownloader",
    "psr/http-factory": "Require to use the provided HttpClientDownloader",
    "psr/http-message": "Require to use the provided HttpClientDownloader"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    }
  },
  "autoload": {
    "psr-4": {
      "Phpcq\\GnuPG\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Phpcq\\GnuPG\\Test\\": "tests/",
      "spec\\Phpcq\\GnuPG\\": "spec/"
    }
  }
}
